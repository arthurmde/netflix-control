{
  "manifest_version": 2,
  "name": "Netflix Remote Control",
  "description": "Server side to use the movile app Netflix Remote Control",
  "version": "0.1",
  "icons": {
    "128": "128.png"
  },
  "app": {
    "urls": [
      "http://certification.googleapps.com/app-info/"
      ],
    "launch": {
      "web_url": "http://certification.googleapps.com/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
